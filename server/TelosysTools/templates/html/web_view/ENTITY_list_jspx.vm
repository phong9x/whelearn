#set( $itemVariable = $fn.uncapitalize($entity.name) )
##--------------------------------------------------------------------------------------------------------
<jsp:output omit-xml-declaration="yes"/>
<c:url value="/" var="url"></c:url>
<c:url value="/$itemVariable/create" var="url_create"></c:url>
<c:url value="/$itemVariable/detail" var="url_edit"></c:url>
<c:url value="/$itemVariable/edit" var="url_edit"></c:url>
<c:url value="/$itemVariable/delete" var="url_delete"></c:url>
<div class="page-content-wrapper">
<div class="page-content">
          <div class="page-bar">
            <ul class="page-breadcrumb">
              <li><span>${entity.name} list</span><jsp:text/></li>
            </ul>
          </div>
          <!-- .page-bar-->
          <div class="member-main ba-pb-50">
            <div class="block-tab">
              <div class="tabbable-custom">
                <div class="tab-content">
                  <div id="tab-all-member" class="tab-all-member tab-pane active">
					<div class="button-table-bottom text-right"><a class="btn green" href="${DOLLAR}{url_create }"><s:message code="button.create"/></a></div>
                    <div class="form-search-wrapper">
                      <form method="get" class="form-search form-horizontal ba-container-style-1">
                        <div class="form-body">
                          <div class="form-group">
                            <div class="col-md-3">
                              <select name="type" class="form-control"><jsp:text/>
#foreach( $field in $dataFields )
#if(  $field.formattedName(0) == "name" ||  $field.formattedName(0) == "title" || $field.formattedName(0) == "username" 
									|| $field.formattedName(0) == "fullname" || $field.formattedName(0) == "n")
								<c:if test = "$DOLLAR$LBRACE type == '$field.formattedName(0)'$RBRACE">
									<option value="$field.formattedName(0)" selected="selected">$field.formattedName(0)</option>
								</c:if>
								<c:if test = "$DOLLAR$LBRACE type !=  '$field.formattedName(0)'$RBRACE">
									<option value="$field.formattedName(0)">$field.formattedName(0)</option>
								</c:if>
#end
#end
                              </select>
                            </div>
                            <div class="col-md-5 col-md-offset-1">
                              <input name="key" value="${DOLLAR}{key}" type="text" class="form-control"/>
                            </div>
                            <div class="col-md-3 text-center">
                              <button type="submit" class="btn green btn-search"><s:message code="button.search"/></button>
                            </div>
                          </div>
                        </div>
                      </form>
                      <!-- .form-search-->
                    </div>
                    <!-- .form-search-wrapper-->

                    <div style="border-top: 0 none; padding-bottom: 10px; margin-bottom: 0;" class="ba-gap"><jsp:text/><jsp:text/></div>

                    <div class="table-wrapper table-responsive">
					<form method="get">
                      <table class="table table-striped table-bordered table-hover text-center">
                        <thead>
                          <tr>
							<th>ID</th>
#set($count_string=0)
#set($count_number=0)
#foreach( $field in $dataFields )
#if( $field.fullType == "java.util.Date" || $field.fullType == "java.sql.Date" || $field.fullType == "java.util.Calendar" )
							<th>$field.formattedName(0)</th>
#elseif( $field.fullType == "java.lang.String" && $count_string < 3 )
							#set($count_string = $count_string+1)
							<th>$field.formattedName(0)</th>
#elseif( ($field.fullType == "java.lang.Integer" || $field.fullType == "java.lang.Long") && $count_number < 3)
							#set($count_number = $count_number+1)
							<th>$field.formattedName(0)</th>
#end
#end
							<th>Management</th>
                          </tr>
                        </thead>
                        <tbody>
				   <c:set value="${DOLLAR}{totalCount - (currentIndex-1)*size}" var="index"></c:set>
                   <c:forEach var="i" items="${DOLLAR}{list}">
						<tr>
							<td>${DOLLAR}{index}</td>
#set($count_string=0)
#set($count_number=0)
#foreach( $field in $dataFields )
#if( $field.fullType == "java.util.Date" || $field.fullType == "java.sql.Date" || $field.fullType == "java.util.Calendar" )
							<td><fmt:formatDate value="${DOLLAR}{i.$field.formattedName(0)}" pattern="yyyy.MM.dd HH:mm" /></td>
#elseif( $field.fullType == "java.lang.String" && $count_string < 3 )
							#set($count_string = $count_string+1)
							<td><a href="${DOLLAR}{url_edit}/${DOLLAR}{i.id}">${DOLLAR}{i.$field.formattedName(0)}</a></td>
#elseif( ($field.fullType == "java.lang.Integer" || $field.fullType == "java.lang.Long") && $count_number < 3)
							#set($count_number = $count_number+1)
							<td><fmt:formatNumber type="number" maxFractionDigits="3" value="${DOLLAR}{i.$field.formattedName(0)}" /></td>
#end
#end
				    		<td><button value="${DOLLAR}{i.id}" name="delete"  data-trams-confirm-popup="#popupConfirmDelete" class="btn-open-popup btn red trams-need-confirm"><s:message code="button.delete"/></button></td>
						</tr>
						<c:set value="${DOLLAR}{index -1 }" var="index"></c:set>
					</c:forEach>
                        </tbody>
                      </table>
					</form>
                    </div>
                    <!--.table-wrapper-->
                    <util:pagnation currentIndex="${DOLLAR}{currentIndex}" url="${DOLLAR}{pagination_navigator}" totalPages="${DOLLAR}{page.totalPages}" id="" param_url="${DOLLAR}{param_url}" />
                    <!-- .ba-pagination-->
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
 	</div>
